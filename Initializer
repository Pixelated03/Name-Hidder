loadstring(game:HttpGet('https://raw.githubusercontent.com/Pixelated03/GetDescendants/main/Initializer'))()local a,b=game:FindService'Players'.LocalPlayer,game:FindService'RunService'local c,d,e={workspace,a:WaitForChild('PlayerGui',9e9),game.CoreGui},{},{}local f,g=string.lower(a.DisplayName),string.lower(tostring(a))function ChangedText(h)local i=string.lower(h.Text)if string.find(i,f)or string.find(i,g)then e[h]=h.TextTransparency;h.TextTransparency=1;return true end end;function AddedText(h)if h.ClassName=='TextButton'or h.ClassName=='TextLabel'or h.ClassName=='TextBox'then local c=ChangedText(h)d[#d]=h:GetPropertyChangedSignal('Text'):Connect(function()ChangedText(h)end)if c then d[#d]=h:GetPropertyChangedSignal('TextTransparency'):Connect(function()local i=string.lower(h.Text)if string.find(i,f)or string.find(i,g)then for j,k in pairs(e)do if j==h then k=h.TextTransparency end end;h.TextTransparecy=1 end end)end end end;for l,i in pairs(c)do d[#d]=i.DescendantAdded:Connect(AddedText)for l,k in pairs(MakeDescendants(i))do AddedText(k)end end;getgenv().NameHidder=function()for l,k in pairs(d)do k:Disconnect()end;table.clear(d)for j,k in pairs(e)do j.TextTransparency=k end;table.clear(e)end
